{
  "schemaVersion": 1,
  "id": "tormod",
  "version": "${version}",

  "name": "TorMod",
  "description": "Use the TOR network to connect to any minecraft server.",
  "authors": ["meo209"],
  "contact": {
    "homepage": "https://github.com/meo209/TorMod",
    "issues": "https://github.com/meo209/TorMod/issues",
    "sources": "https://github.com/meo209/TorMod"
  },

  "license": "GPL-3.0",
  "icon": "assets/tormod/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": ["com.kvxd.tormod.TorMod"],
    "modmenu": [
      "com.kvxd.tormod.config.ModMenuIntegration"
    ]
  },

  "mixins": [
    "tormod.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-language-kotlin": ">=${kotlin_loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": ">=17.0.144"
  }
}
